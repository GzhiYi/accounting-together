.group-dialog-body {
  padding: 40rpx 0;
}
page {
}
.new-item {
  width:80%;
  background-color: #fff;
  border-radius:38rpx;
  height:200rpx;
  text-align: center;
  margin: 40rpx auto;
  box-shadow: 0px 7px 20px 4px #ececec;
  display:flex;
  justify-content:space-between;
  padding: 20rpx;
  transition: .5s all;
}
.new-item image {
  width:124rpx;
  height:124rpx;
}
.right-conner {
  display:flex;
  flex-direction:column;
  justify-content:space-between;
}
.right-conner .item-name {
  margin-right:14rpx;
}
.icon-arrow {
  text-align:right;
  padding-right:13rpx;
}
.new-item:hover {
  box-shadow: none;
}